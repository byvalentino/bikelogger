{
  "expo": {
    "name": "BikeLogger",
    "slug": "BikeLogger",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "org.uk.bikelogger"
    },
    "ios": {
      "supportsTablet": true
    },
    "description": "BikeLogger collects cycling travel data. The data is then used to better plan our cycling infrastructure."
  }
}
